{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"name": "wraththeme",
	"colors": {
		"editor.background": "#101010",
		"editor.foreground": "#f1f1ef",
		"sideBar.background": "#101010",
		"tab.inactiveBackground":"#0e0e0e",
		"editorGroupHeader.tabsBackground":"#0e0e0e",
		"editorGroupHeader.noTabsBackground":"#0e0e0e",
		"titleBar.inactiveBackground":"#0e0e0e",
		"titleBar.activeBackground":"#0e0e0e",
		"activityBar.background":"#0e0e0e",
		"activityBarBadge.background": "#0e0e0e",
		"statusBar.background":"#0e0e0e",
		"statusBar.noFolderBackground": "#0e0e0e",
		"sideBarTitle.foreground": "#bbbbbb",
		"git.blame.editorDecorationForeground": "#969696",
		"gitDecoration.addedResourceForeground": "#81b88b",
		"gitDecoration.conflictingResourceForeground": "#e4676b",
		"gitDecoration.deletedResourceForeground": "#39c763",
		"gitDecoration.ignoredResourceForeground": "#8c8c8c",
		"gitDecoration.modifiedResourceForeground": "#f8efc8",
		"gitDecoration.renamedResourceForeground": "#73c991",
		"gitDecoration.stageDeletedResourceForeground": "#c74e39",
		"gitDecoration.stageModifiedResourceForeground": "#e2c08d",
		"gitDecoration.submoduleResourceForeground": "#8db9e2",
		"gitDecoration.untrackedResourceForeground": "#88bb9a",
		"editorGutter.addedBackground": "#E9C0B4",
		"editorGutter.addedSecondaryBackground": "#243f01",
		"editorGutter.modifiedBackground": "#EEEBD3",
		"editor.selectionBackground": "#5e4d4d",
		"editorLineNumber.activeForeground": "#E9C0B4",
		"editorLineNumber.foreground": "#858585",
		"icon.foreground": "#c5c5c5",
		"editorGutter.deletedBackground": "#E9C0B4"
	},
	"tokenColors": [
		{
			"scope": "source",
			"settings": {
				"foreground": "#fcf6ef"
			}
		},
		{
			"scope": ["keyword.control", "keyword.other.nim", "storage.modifier.odin"],
			"settings": {
				"foreground": "#eeebd3",
			}
		},
		{
			"scope": ["entity.name.function", "support.function.builtin", "support.function.any-method"],
			"settings": {
				"foreground": "#eeddd3",
			}
		},
		{
			"scope": ["keyword.operator", "support.type", "storage.type", "entity.name.type"],
			"settings": {
				"foreground": "#e9c0b4"
			}
		},
		{
			"scope": "string.quoted",
			"settings": {
				"foreground": "#f8efc8"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language"],
			"settings": {
				"foreground": "#ddb6a8"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#a09690"
			}
		},
		{
			"scope": "keyword.context",
			"settings": {
				"foreground": "#f5ecc6",
			}
		},
		{
			"scope": "entity.name.type.module.odin",
			"settings": {
				"foreground": "#dfa8a8",
			}
		},


		// # Directives
		{
			"scope": "entity.name.tag.odin",
			"settings": {
				"foreground": "#e6e9b9",
			}
		},

		// XML tags 
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#e6e9b9"
			}
		},

	]
	
}
